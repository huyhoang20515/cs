using System;

public class AgeOutOfRangeException : Exception
{
    public AgeOutOfRangeException(string message) : base(message)
    {
    }
}

public class Program
{
    public static void Main(string[] args)
    {
        Console.WriteLine("Nhap tuoi cua sinh vien:");
        int age = Convert.ToInt32(Console.ReadLine());

        try
        {
            Kiemtradieukien(age);
            Console.WriteLine("Sinh vien du dieu kien nhap hoc.");
        }
        catch (AgeOutOfRangeException ex)
        {
            Console.WriteLine(ex.Message);
        }
    }

    public static void Kiemtradieukien(int age)
    {
        if (age < 18 || age > 25)
        {
            Console.WriteLine("Tuoi phai nam trong khoang tu 18 den 25 ( bao gom 18 va 25) de du dieu kien nhap hoc.");
        }
    }
}
